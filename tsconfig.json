{
  "compilerOptions": {
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "es6",
      "es2016.array.include",
      "es2017.object",
      "dom"
    ],
    "target": "es5",
    "baseUrl": "./src",
    "paths": {
      "app/*": [
        "main/app/*"
      ],
      "features/*": [
        "main/features/*"
      ],
      "client/*": [
        "main/app/client/*"
      ],
      "drafts/*": [
        "main/app/drafts/*"
      ],
      "claims/*": [
        "main/app/claims/*"
      ],
      "fees/*": [
        "main/app/fees/*"
      ],
      "forms/*": [
        "main/app/forms/*"
      ],
      "health/*": [
        "main/app/health/*"
      ],
      "idam/*": [
        "main/app/idam/*"
      ],
      "logging/*": [
        "main/app/logging/*"
      ],
      "models/*": [
        "main/app/models/*"
      ],
      "utils/*": [
        "main/app/utils/*"
      ],
      "modules/*": [
        "main/modules/*"
      ],
      "routes/*": [
        "main/routes/*"
      ],
      "common/*": [
        "main/common/*"
      ],
      "claim/*": [
        "main/features/claim/*"
      ],
      "first-contact/*": [
        "main/features/first-contact/*"
      ],
      "response/*": [
        "main/features/response/*"
      ],
      "offer/*": [
        "main/features/offer/*"
      ],
      "statement-of-means/*": [
        "main/features/statement-of-means/*"
      ],
      "dashboard/*": [
        "main/features/dashboard/*"
      ],
      "ccj/*": [
        "main/features/ccj/*"
      ],
      "testing-support/*": [
        "main/features/testing-support/*"
      ],
      "services/*": [
        "main/services/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  }
}
